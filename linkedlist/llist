class LList{
 Node head;
 class Node{
  int data;
  Node next; 

  public void Node(int n){
   data = n;
  }
 }

 public static void main(String args[]){
  LList ll = new LList();
  Ll.head = new Node(1);
  Node second = new Node(2);
  Node third = new Node(3);

  ll.head.next = sencod;
  second.next = thrid;
